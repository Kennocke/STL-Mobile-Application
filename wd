[32m+[m[32m﻿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.IO;[m
[32m+[m
[32m+[m[32mnamespace ConsoleApp1[m
[32m+[m[32m{[m
[32m+[m[32m    class Program[m
[32m+[m[32m    {[m
[32m+[m[41m        [m
[32m+[m[32m        static void Main(string[] args)[m
[32m+[m[32m        {[m
[32m+[m[32m            double[] normal = new double[3];[m
[32m+[m[32m            double[] vertex1 = new double[3];[m
[32m+[m[32m            double[] vertex2 = new double[3];[m
[32m+[m[32m            double[] vertex3 = new double[3];[m
[32m+[m[32m            ushort count;[m
[32m+[m[32m            double sum = 0;[m
[32m+[m[32m            byte[] header = new byte[80];[m
[32m+[m[41m            [m
[32m+[m[32m            using (BinaryReader reader = new BinaryReader(File.Open("file.stl", FileMode.Open)))[m
[32m+[m[32m            {[m
[32m+[m[32m                FileStream file1 = new FileStream("test.txt", FileMode.Create);[m
[32m+[m[32m                StreamWriter writer = new StreamWriter(file1);[m
[32m+[m[32m                reader.BaseStream.Seek(84,SeekOrigin.Begin);[m
[32m+[m[32m                //number = reader.ReadUInt32();[m
[32m+[m[32m                while(reader.BaseStream.Position != reader.BaseStream.Length)[m
[32m+[m[32m                {[m
[32m+[m[32m                        for (int i = 0; i < 3; i++) normal[i] = reader.ReadSingle() * 10;[m
[32m+[m[32m                        for (int i = 0; i < 3; i++) vertex1[i] = reader.ReadSingle() * 0.01;[m
[32m+[m[32m                        for (int i = 0; i < 3; i++) vertex2[i] = reader.ReadSingle() * 0.01;[m
[32m+[m[32m                        for (int i = 0; i < 3; i++) vertex3[i] = reader.ReadSingle() * 0.01;[m
[32m+[m[32m                        count = reader.ReadUInt16();[m
[32m+[m[32m                        sum =+ ((-1) * vertex3[0] * vertex2[1] * vertex1[2] + vertex2[0] * vertex3[1] * vertex1[2] + vertex3[0] * vertex1[1] * vertex2[2] - vertex1[0] * vertex3[1] * vertex2[2] - vertex2[0] * vertex1[1] * vertex3[2] + vertex1[0] * vertex2[1] * vertex3[2]) / 6;[m
[32m+[m[32m                        writer.WriteLine("  facet normal " + normal[0] + " " + normal[1] + " " + normal[2]);[m
[32m+[m[32m                        writer.WriteLine("    outer loop");[m
[32m+[m[32m                        writer.WriteLine("      vertex " + vertex1[0] + " " + vertex1[1] + " " + vertex1[2]);[m
[32m+[m[32m                        writer.WriteLine("      vertex " + vertex2[0] + " " + vertex2[1] + " " + vertex2[2]);[m
[32m+[m[32m                        writer.WriteLine("      vertex " + vertex3[0] + " " + vertex3[1] + " " + vertex3[2]);[m
[32m+[m[32m                        writer.WriteLine("    endloop");[m
[32m+[m[32m                        writer.WriteLine("  endfacet");[m
[32m+[m[32m                }[m
[32m+[m[41m                [m
[32m+[m[32m                Console.WriteLine(sum*1.25);[m
[32m+[m[32m                Console.ReadLine();[m
[32m+[m[32m                writer.Close();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
